<div>
  <%= form_with url: search_path, method: :get do |form| %>
    <%= form.label :query, "Search for:" %>
    <%= form.text_field :query %>
    <%= form.submit "Search" %>
  <%end%>
  <% @search_user.each do |user| %>
    <p>
      <%= user.id %>
      <%= user.nickname %>
    </p>
  <% end %>
</div>
